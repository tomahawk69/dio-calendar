<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dio.calendar.EntryWrapper" table="entries">
        <id column="f_id" name="id" type="uuid-char">

        </id>
        <property column="f_subject" name="subject" type="java.lang.String"/>
        <property column="f_description" name="description" type="java.lang.String"/>

    </class>
</hibernate-mapping>